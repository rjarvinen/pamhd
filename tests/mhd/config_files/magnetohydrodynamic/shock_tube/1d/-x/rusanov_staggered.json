{
	"output-directory": "results/magnetohydrodynamic/shock_tube/1d/-x/rusanov_staggered",
	"time-start": 0,
	"time-length": 1,
	"minimum-pressure": 0,
	"solver-mhd": "rusanov",
	"load-balancer": "RANDOM",
	"save-mhd-n": 0.1,
